# LM Studio Configuration
LMSTUDIO_MODEL=gemma3:27b
LMSTUDIO_HOST=0.0.0.0
LMSTUDIO_PORT=1234

# Nginx Configuration
NGINX_PORT=8080
NGINX_SSL_PORT=8443

# Ngrok Configuration
NGROK_AUTHTOKEN=your_ngrok_auth_token_here
NGROK_REGION=us

# Authentication
AUTH_USERNAME=admin
AUTH_PASSWORD=secure_password_123

# Rate Limiting
RATE_LIMIT=10r/s
RATE_BURST=20

# Nginx Timeout Configuration
NGINX_PROXY_CONNECT_TIMEOUT=90
NGINX_PROXY_SEND_TIMEOUT=330
NGINX_PROXY_READ_TIMEOUT=330

# SSL Configuration (optional)
SSL_ENABLED=false
SSL_CERT_PATH=./certs/server.crt
SSL_KEY_PATH=./certs/server.key
